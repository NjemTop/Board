{
    "swagger": "2.0",
    "info": {
      "title": "Описание API",
      "description": "API для всего вэб-сервера.",
      "version": "1.3"
    },
    "basePath": "/",
    "schemes": [
      "http"
    ],
    "paths": {
      "/data_release/api/versions": {
        "get": {
          "summary": "Get API data release versions",
          "description": "Get all available data release versions.",
          "responses": {
            "200": {
              "description": "Successful operation"
            }
          }
        }
      },
      "/data_release/api/{version}": {
        "get": {
          "summary": "Get API data release number",
          "description": "Get data release information for the specified version.",
          "parameters": [
            {
              "name": "version",
              "in": "path",
              "description": "Data release version",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "200": {
              "description": "Successful operation"
            },
            "404": {
              "description": "Version not found"
            }
          }
        }
      }
    }
  }
  